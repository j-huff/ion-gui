  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "deep-diff": "^1.0.2",
    "express": "^4.16.2",
    "reactstrap": "^6.5.0",
    "regex": "^0.1.1",
    "shortid": "^2.2.14",
    "sqlite3": "^4.0.6"
  }
}
